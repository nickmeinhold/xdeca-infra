# Copy to secrets.yaml and fill in values, then encrypt with:
# sops -e -i secrets.yaml

# Webhook server secrets
webhook_secret: your-openproject-webhook-secret
gcal_webhook_secret: your-google-calendar-webhook-secret

# OpenProject API
openproject_url: https://your-openproject-instance.com
openproject_api_key: your-openproject-api-key

# Google Calendar
google_calendar_id: your-calendar-id@group.calendar.google.com
google_service_account_json: '{"type":"service_account",...}'
